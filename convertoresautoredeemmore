

getgenv().AutoRedeemActiveSIKUHUB = false
getgenv().AutoPickupActiveSIKUHUB = false
getgenv().AutoConvertPotionsSIKUHUB = false
getgenv().AmountToConvertSIKUHUB = 0
local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/deeeity/mercury-lib/master/src.lua"))()
local MainWindow = Mercury:Create{
    Name = "Dartanian wants me fr",
    Size = UDim2.fromOffset(400, 450),
    Theme = Mercury.Themes.Dark,
    Link = "https://github.com/"
}
local MainTab = MainWindow:Tab{
	Name = "Main",
	Icon = "rbxassetid://8569322835"
}
local activateautoequip = function()
    local bag = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Wen")
    game.Players.LocalPlayer.Character:FindFirstChild("Humanoid"):EquipTool(bag) 
end
local activateautoredeem = function()
    while true do 
        wait()
        local tool = game:GetService("Players").LocalPlayer.Character:FindFirstChild("Wen")
        if AutoRedeemActiveSIKUHUB == true then 
            if tool then 
                if AutoRedeemActiveSIKUHUB == true then 
                    tool:Activate()
                    if AutoConvertPotionsSIKUHUB == true then 
                        local args3 = {
                            [1] = 0,
                            [2] = 300,
                            [3] = 0
                        }
                        
                        game:GetService("ReplicatedStorage").purchase_elixirs:FireServer(unpack(args3))
                    end
                elseif AutoRedeemActiveSIKUHUB == false then break
                end
            elseif not tool then 
                activateautoequip()
            end
        elseif AutoRedeemActiveSIKUHUB == false then break 
        end 
    end    
end
local activateautopickup = function()
    while true do 
        if AutoPickupActiveSIKUHUB == true then 
            wait(1)
            if workspace:FindFirstChild("Money bag") then
                local name2 = tostring(game:GetService("Players").LocalPlayer.Name)
                print(name2)
                local args = {
                    [1] = "transfer_money_to_money_bag2",
                    [2] = game:GetService("Players").LocalPlayer,
                    [3] = game:GetService("ReplicatedStorage").Player_Data:FindFirstChild(name2),
                    [4] = workspace:FindFirstChild("Money bag")
                }
                game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer(unpack(args))
            end
        elseif AutoPickupActiveSIKUHUB == false then break 
        end
    end
end









-- Script generated by SimpleSpy - credits to exx#9394




local AutoRedeemToggle = MainTab:Toggle{
	Name = "Auto Redeem Money Bags",
	StartingState = false,
	Description = "this will equip and reedeem money bags  ",
	Callback = function(state)
        print(state)
        getgenv().AutoRedeemActiveSIKUHUB = state
        activateautoredeem()
        activateautoequip()
    
    end
}
local AutoPickup = MainTab:Toggle{
	Name = "Auto Pickup",
	StartingState = false,
	Description = "this will pickup nearby money bags",
	Callback = function(state)
        print(state)
        getgenv().AutoPickupActiveSIKUHUB = state
        activateautopickup()
    
    end
}

local AutoConvertToPotions = MainTab:Toggle{
	Name = "Enable Auto Convert",
	StartingState = false,
	Description = "auto buys 300 potions (ENABLE AUTO REDEEM TO USE)!!!",
	Callback = function(state)
        print(state)
        getgenv().AutoConvertPotionsSIKUHUB = state
    
    end
}
local amoutnofpotions = MainTab:Textbox{
    Name = "Stamina Elixers",
    Description = "enter the amount of stamina pots want to convert",
    Callback = function(text)
        AmountToConvertSIKUHUB = tonumber(text)
    end
}
local convertpotionstoores = MainTab:Button{
	Name = "Convert Potions To Ores",
	Description = "Enter the amount of stamina pots u have above",
	Callback = function() 
        local amountoforespurchasable = AmountToConvertSIKUHUB / 86.5
        local args = {
            [1] = {
                ["Stamina Elixir"] = AmountToConvertSIKUHUB
            },
            [2] = math.floor(amountoforespurchasable),
            [3] = 0
        }
        
        game:GetService("ReplicatedStorage").Sell_Items_tang:InvokeServer(unpack(args))
    end
}
